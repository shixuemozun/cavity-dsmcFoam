FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "20.5";
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    FLOW
    {
        type            fixedValue;
        value           uniform (50 0 0);
    }
    WALL
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
}


